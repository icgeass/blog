#set($layout='admin/layout/layout.vm')

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h3 class="page-header">文件上传</h3>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <form id="pageform" class="form-inline" action="${request.contextPath}/admin/attach/save" method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="col-lg-12">
                <label class="btn btn-default">
                    Browse <input type="file" name="file" hidden >
                </label>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <input class="btn btn-default" type="submit" value="Submit">
            </div>
            <!-- /.col-lg-12 -->
        </div>
    </form>
</div>
